 * @author CHUA WEE CHAN 271948
 */
public class Tablet1 {
    String Brand;
    String OperatingSystem;
    int Year;
    String Size;
    
    void printIpad(){
        Brand="Apple";
        OperatingSystem="ios";
        Year=2019;
        Size="9.8inch x 6.8inch";
        
        System.out.println("The brand of the ipad is "+Brand);
        System.out.println("The operating system of the ipad is "+OperatingSystem);
        System.out.println("The release year of the ipad is "+Year);
        System.out.println("The size of the ipad is "+Size);
        
    }
    
}
